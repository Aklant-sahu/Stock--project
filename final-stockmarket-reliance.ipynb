    "def buysell(data):\n",
    "    act=[]\n",
    "    for i in range(len(data)):\n",
    "        \n",
    "        if (data['actual_close'][i] > data['prev_Close'][i]):\n",
    "            act.append(1)\n",
    "        else:\n",
    "            act.append(0)\n",
    "\n",
    "    data['action']=act\n",
    "    act=[]\n",
    "    for i in range(len(data)):\n",
    "        \n",
    "        if (data['pred_close'][i] > data['prev_Close'][i]):\n",
    "            act.append(1)\n",
    "        else:\n",
    "            act.append(0)\n",
    "\n",
    "    data['action_pred']=act\n",
    "    \n",
    "    #data=data.drop('Close',axis=1)\n",
    "    return data"
   ]
